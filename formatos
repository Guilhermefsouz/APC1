#include <stdio.h>

int main(){
	
	int x;
	int y;
	int z;
	
	printf("primeiro lado: ");
	scanf("%d", &x);
	
	printf("segundo lado: ");
	scanf("%d", &y);
	
	printf("terceiro lado: ");
	scanf("%d", &z);
	
	if((x == y && y != z) || (x != y && y == z) || (z == x && x != y)){
		printf("tringulo isoceles (dois lados iguais)");
	}
	
	else if(x == y == z){
		printf("triangulo equilatero (tres lados iguais)");
	}
	
	else if(x != y != z){
		printf("triangulo escaleno (nenhum lado igual)");
	}
	
	return 0;
}
