#include <stdio.h>
#include <stdlib.h>

int main (){
	
	int a = 0;
	int b = 0;
	int c = 0;
	
	printf("Digite p valor de a: ");
	scanf("%d", &a);
	
	printf("Digite o valor de b: ");
	scanf("%d", &b);
	
	for (c = a; c <= b; c++){
		
		printf("%d \n valor atual de c: ", c);
	}
	
	system("pause");
	
	return 0;
}
